	<parameter name="fechaCierreCeniaq" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    cve_art,
    unidad,
    substr(des_articulo,1,50) as des_articulo,
    (NVL(
    (
        SELECT
            SUM(ed.cantidad)
        FROM
            entrada_ceye e,
            entrada_detalle_ceye ed
        WHERE
            e.id_entrada = ed.id_entrada
        AND e.fecha_entrada >  $P{fechaCierreCeye}
        AND ed.cve_art = artExt.cve_art
        AND e.estado_entrada='A'
        AND e.almacen = 'C'
        GROUP BY
            ed.cve_art
    )
    ,0)+
     NVL(
    (
        SELECT
            cc.existencia
        FROM
            cierre_ceye cc
        WHERE
            fecha_cierre = $P{fechaCierreCeye}
        AND cc.cve_art = artExt.cve_art
        AND almacen = 'C'
    )
    ,0)) as entradaceye,
 NVL(
    (
        SELECT
            SUM(sd.cantidad_surtida)
        FROM
            salida_ceye s,
            salida_detalle_ceye sd
        WHERE
            s.id_salida = sd.id_salida
        AND s.fecha_salida > $P{fechaCierreCeye}
        AND sd.cve_art = artExt.cve_art
        AND s.estado_salida = 'A'
        AND s.almacen = 'C'
        GROUP BY
            sd.cve_art
    )
    ,0) as salidaceye,
(NVL(
    (
        SELECT
            SUM(ed.cantidad)
        FROM
            entrada_ceye e,
            entrada_detalle_ceye ed
        WHERE
            e.id_entrada = ed.id_entrada
        AND e.fecha_entrada > $P{fechaCierreSubceye}
        AND ed.cve_art = artExt.cve_art
        AND e.estado_entrada='A'
        AND e.almacen = 'S'
        GROUP BY
            ed.cve_art
    )
    ,0)+
     NVL(
    (
        SELECT
            cc.existencia
        FROM
            cierre_ceye cc
        WHERE
            fecha_cierre = $P{fechaCierreSubceye}
        AND cc.cve_art = artExt.cve_art
        AND almacen = 'S'
    )
    ,0)) as entradasubceye,
 NVL(
    (
        SELECT
            SUM(sd.cantidad_surtida)
        FROM
            salida_ceye s,
            salida_detalle_ceye sd
        WHERE
            s.id_salida = sd.id_salida
        AND s.fecha_salida > $P{fechaCierreSubceye}
        AND sd.cve_art = artExt.cve_art
        AND s.estado_salida = 'A'
        AND s.almacen = 'S'
        GROUP BY
            sd.cve_art
    )
    ,0) as salidasubceye,
(NVL(
    (
        SELECT
            SUM(ed.cantidad)
        FROM
            entrada_ceye e,
            entrada_detalle_ceye ed
        WHERE
            e.id_entrada = ed.id_entrada
        AND e.fecha_entrada > $P{fechaCierreCeniaq}
        AND ed.cve_art = artExt.cve_art
        AND e.estado_entrada='A'
        AND e.almacen = 'Q'
        GROUP BY
            ed.cve_art
    )
    ,0)+
     NVL(
    (
        SELECT
            cc.existencia
        FROM
            cierre_ceye cc
        WHERE
            fecha_cierre = $P{fechaCierreCeniaq}
        AND cc.cve_art = artExt.cve_art
        AND almacen = 'Q'
    )
    ,0)) as entradaceniaq,
NVL(
    (
        SELECT
            SUM(sd.cantidad_surtida)
        FROM
            salida_ceye s,
            salida_detalle_ceye sd
        WHERE
            s.id_salida = sd.id_salida
        AND s.fecha_salida > $P{fechaCierreCeniaq}
        AND sd.cve_art = artExt.cve_art
        AND s.estado_salida = 'A'
        AND s.almacen = 'Q'
        GROUP BY
            sd.cve_art
    )
    ,0) as salidaceniaq
FROM
    articulo_ceye artExt
WHERE
    1 = 1 $P!{insumosValue} $P!{partidaValue}
ORDER BY artExt.cve_art]]>
	</queryString>
